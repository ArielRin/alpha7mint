pragma solidity ^0.8.0;


interface NFTContract {
    function getTokenDetails(uint256 tokenId) external view returns (string memory, string memory, string memory, string memory);
    function getPreviousOwners(uint256 tokenId) external view returns (address[] memory);
}

interface BattleContract {
    function getBattleStats(uint256 tokenId) external view returns (uint256, uint256, uint256, uint256[] memory, uint256);
}

interface ERC20Token {
    function transfer(address recipient, uint256 amount) external returns (bool);
    function transferFrom(address sender, address recipient, uint256 amount) external returns (bool);
    function allowance(address owner, address spender) external view returns (uint256);
}

contract AlphaDawgRegistration {
    using ABDKMath64x64 for int128;

    address public owner;
    address public nftContractAddress;
    address public battleContractAddress;
    address public ercTokenAddress;
    address public nftTreasuryAddress;
    uint256 public registrationFee;
    uint256 public modificationFee;
    string public baseURI;

    mapping(uint256 => bool) public isRegistered;
    mapping(uint256 => mapping(address => uint256)) public registrationDate;
    mapping(uint256 => uint256) public modificationCount;
    mapping(uint256 => uint256) public registrationCount;
    mapping(uint256 => string) public dawgzNames;
    mapping(uint256 => string) public dawgzDefaultTaunts;
    mapping(uint256 => string) public dawgzBios;

    modifier onlyOwner() {
        require(msg.sender == owner, "Only owner can call this function");
        _;
    }

    modifier onlyNFTOwner(uint256 tokenId) {
        require(NFTContract(nftContractAddress).getPreviousOwners(tokenId)[0] == msg.sender, "You are not the owner of this NFT.");
        _;
    }

    constructor(
        address _nftContractAddress,
        address _battleContractAddress,
        address _ercTokenAddress,
        address _nftTreasuryAddress,
        uint256 _registrationFee,
        uint256 _modificationFee,
        string memory _baseURI
    ) {
        owner = msg.sender;
        nftContractAddress = _nftContractAddress;
        battleContractAddress = _battleContractAddress;
        ercTokenAddress = _ercTokenAddress;
        nftTreasuryAddress = _nftTreasuryAddress;
        registrationFee = _registrationFee;
        modificationFee = _modificationFee;
        baseURI = _baseURI;
    }

    function registerNFT(uint256 tokenId, string memory dawgzName, string memory dawgzDefaultTaunt, string memory dawgzBio) external payable {
        require(!isRegistered[tokenId], "NFT is already registered.");
        require(msg.value >= registrationFee, "Insufficient funds.");

        (bool success, ) = nftTreasuryAddress.call{value: msg.value}("");
        require(success, "Transfer to treasury failed.");

        isRegistered[tokenId] = true;
        registrationDate[tokenId][msg.sender] = block.timestamp;
        registrationCount[tokenId]++;

        dawgzNames[tokenId] = dawgzName;
        dawgzDefaultTaunts[tokenId] = dawgzDefaultTaunt;
        dawgzBios[tokenId] = dawgzBio;
    }

    function modifyNFTDetails(uint256 tokenId, string memory newName, string memory newTaunt, string memory newBio) external onlyNFTOwner(tokenId) payable {
        require(msg.value >= modificationFee, "Insufficient funds.");

        (bool success, ) = nftTreasuryAddress.call{value: msg.value}("");
        require(success, "Transfer to treasury failed.");

        // Modify NFT details here

        registrationDate[tokenId][msg.sender] = block.timestamp;
        modificationCount[tokenId]++;
    }

    function transferOwnership(uint256 tokenId, address newOwner) external onlyNFTOwner(tokenId) {
        require(newOwner != address(0), "Invalid new owner address");

        // Transfer ownership logic here
    }

    function recoverTokens(address tokenAddress, address to, uint256 amount) external onlyOwner {
        require(tokenAddress != address(0), "Invalid token address");
        require(to != address(0), "Invalid recipient address");

        ERC20Token(tokenAddress).transfer(to, amount);
    }

    function recoverNativeTokens(address to, uint256 amount) external onlyOwner {
        require(to != address(0), "Invalid recipient address");

        (bool success, ) = to.call{value: amount}("");
        require(success, "Recovery transfer failed");
    }

    function setRegistrationFee(uint256 _registrationFee) external onlyOwner {
        registrationFee = _registrationFee;
    }

    function setModificationFee(uint256 _modificationFee) external onlyOwner {
        modificationFee = _modificationFee;
    }

    function setBaseURI(string memory _baseURI) external onlyOwner {
        baseURI = _baseURI;
    }

    function isNFTRegistered(uint256 tokenId) external view returns (bool) {
        return isRegistered[tokenId];
    }

    function getRegistrationDate(uint256 tokenId, address owner) external view returns (uint256) {
        return registrationDate[tokenId][owner];
    }

    function getModificationCount(uint256 tokenId) external view returns (uint256) {
        return modificationCount[tokenId];
    }

    function getRegistrationCount(uint256 tokenId) external view returns (uint256) {
        return registrationCount[tokenId];
    }

    function getDawgzDetails(uint256 tokenId) external view returns (string memory) {
        require(isRegistered[tokenId], "NFT is not registered");

        (string memory dawgzName, string memory dawgzTaunt, string memory dawgzBio, string memory breed) = NFTContract(nftContractAddress).getTokenDetails(tokenId);

        string memory details = string(
            abi.encodePacked(
                "Dawgz Name: ", dawgzName, "\n",
                "Dawgz Taunt: ", dawgzTaunt, "\n",
                "Dawgz Bio: ", dawgzBio, "\n",
                "Breed: ", breed, "\n" // Added breed attribute
            )
        );

        return details;
    }

function getBattleDetails(uint256 tokenId) external view returns (string memory) {
        require(isRegistered[tokenId], "NFT is not registered");

        (uint256 battlesWon, uint256 battlesLost, uint256 totalBattles, uint256[] memory battleIds, uint256 totalPrizesWon) = BattleContract(battleContractAddress).getBattleStats(tokenId);

        string memory details = string(
            abi.encodePacked(
                "Battles Won: ", uint2str(battlesWon), "\n",
                "Battles Lost: ", uint2str(battlesLost), "\n",
                "Total Battles: ", uint2str(totalBattles), "\n",
                "Total Prizes Won: ", uint2str(totalPrizesWon), "\n"
            )
        );

        return details;
    }


    function uint2str(uint256 _i) internal pure returns (string memory) {
        if (_i == 0) {
            return "0";
        }
        uint256 j = _i;
        uint256 len;
        while (j != 0) {
            len++;
            j /= 10;
        }
        bytes memory bstr = new bytes(len);
        uint256 k = len;
        while (_i != 0) {
            k = k - 1;
            uint8 temp = (48 + uint8(_i - _i / 10 * 10));
            bytes1 b1 = bytes1(temp);
            bstr[k] = b1;
            _i /= 10;
        }
        return string(bstr);
    }
}
